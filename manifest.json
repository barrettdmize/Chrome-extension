/*built using the google tutorial for chrome-extension*/

{
  "manifest_version": 2,

  "name": "Top World News",
  "description": "This extension shows all the top headlines for the worldnews section of reddit",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
